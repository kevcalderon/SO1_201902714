PARA ACCEDER A BASE DE DATOS IT
sudo docker pull mysql
sudo docker run --name practica1db -p 3306:3306 -e MYSQL_ROOT_PASSWORD=root -d mysql
sudo docker exec -it practica1db mysql -uroot -proot

PARA CREAR IMAGEN PARA API
sudo docker build -t practica1api .
sudo docker run -p 8080:8080  practica1api



QUERY DATABASE
create database sopes1practica1db;
use sopes1practica1db;

create table Log(
    idLog integer auto_increment primary key,
    descripcion varchar(255) not null
);

insert into Log(descripcion) values("se hizo la operacion suma");

select * from Log;

